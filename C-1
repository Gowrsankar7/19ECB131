#include <stdio.h>

int main() {
    int a;
    printf("Enter an integer (positive or negative): ");
    scanf("%d", &a);
    if (a > 0) {
      printf("The nubmber %d is positive", a);
      if (a %5 == 0)
        printf("the number is divid by 5");
      else
        printf("the number is not divid by 5");
    }
    else if(a < 0)
      printf("The number %d is negative");
    
    return 0;
}

//out put 

